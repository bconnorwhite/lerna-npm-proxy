    "test": "bob test"
  },
  "dependencies": {
    "@bconnorwhite/exec": "^6.0.8",
    "chalk": "^4.1.1",
    "commander-version": "^2.0.0",
    "fastify": "^3.19.0",
    "fastify-http-proxy": "^6.0.0",
    "package-run": "^2.0.6",
    "pacote": "^11.3.5",
    "parse-json-object": "^2.0.1",
    "sha": "^3.0.0",
    "which-pm-lockfile": "^1.1.1"
  },
  "peerDependencies": {
    "lerna": "^4.0.0"
  },
  "devDependencies": {
    "@bconnorwhite/bob": "^2.9.4",
    "@types/node": "^16.3.1",
    "@types/pacote": "^11.1.1",
    "@types/sha": "^3.0.1"
  },
  "eslintConfig": {
    "extends": "eslint-config-bob"
  },
  "husky": {
    "hooks": {
